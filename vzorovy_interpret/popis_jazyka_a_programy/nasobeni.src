<deklarace promennych>
a;
b;
c;
d;
r;

<program provede nasobeni dvou celych nezapornych cisel a, b>
{
  read a;
  read b;
  setzero c;
  setzero d;
  setzero r;
  
  <provedeme kopii b do c, d>
  while b
  {
    --b;
    ++c;
    ++d;
  }
  
  <provedeme samotne nasobeni>
  while a
  {
    while c
    {
      ++r;
      --c;
    }
    
    while d
    {
      --d;
      ++b;
      ++c;
    }
    
    while b
    {
      --b;
      ++d;
    }
    --a;
  }
  write r;
}
